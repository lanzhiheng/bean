a = 10
b = 10e3
c = 11.111e3
assert(b, 10000)
assert(c, 11111)

a = 10.22
assert(a.toFixed(1), '10.2')
assert(a.toString(), '10.220000')

b = 10000000
assert(b.toFixed(4), '10000000.0000')
assert(b.toString(), '10000000')

c = 999.00999
assert(c.toFixed(), '999.01')
assert(c.toFixed(-1), '999.0')
assert(c.toFixed(-10), '999.0099900000')

e = 999.00999
assert(e.toExponential(), '9.990100e+02')

plusplusA = 100
assert(++plusplusA, 101)
assert(plusplusA++, 101)
assert(plusplusA, 102)

subsubA = 200
assert(--subsubA, 199)
assert(subsubA--, 199)
assert(subsubA, 198)

a = 100
a += 10
assert(a, 110)
a -= 10
assert(a, 100)
a *= 10
assert(a, 1000)
a /= 10
assert(a, 100)

a = 2
a |= 10
assert(a, 10)
a = 2
a &= 10
assert(a, 2)
a = 2
a %= 10
assert(a, 2)
a = 2
a ^= 10
assert(a, 8)
