var a = 10
var b = 10e3
var c = 11.111e3
assert(b, 10000)
assert(c, 11111)

var a = 10.22
assert(a.toFixed(1), '10.2')
assert(a.toString(), '10.220000')

var b = 10000000
assert(b.toFixed(4), '10000000.0000')
assert(b.toString(), '10000000')

var c = 999.00999
assert(c.toFixed(), '999.01')
assert(c.toFixed(-1), '999.0')
assert(c.toFixed(-10), '999.0099900000')

var e = 999.00999
assert(e.toExponential(), '9.990100e+02')

var plusplusA = 100
assert(++plusplusA, 101)
assert(plusplusA++, 101)
assert(plusplusA, 102)

var subsubA = 200
assert(--subsubA, 199)
assert(subsubA--, 199)
assert(subsubA, 198)

var a = 100
a += 10
assert(a, 110)
a -= 10
assert(a, 100)
a *= 10
assert(a, 1000)
a /= 10
assert(a, 100)

var a = 2
a |= 10
assert(a, 10)
a = 2
a &= 10
assert(a, 2)
a = 2
a %= 10
assert(a, 2)
a = 2
a ^= 10
assert(a, 8)

assert(1 + 2 and 3, 3)
assert(1 + 2 or 0, 3)
assert(1 + 2 or false, 3)
assert(1 >> 3, 0)
assert(1 << 2, 4)
assert(1.3 << 4, 16)

assert(not (nil and true), false)
assert(not (1 + 2 + 3 and true), false)

assert(4 | 1, 5)
assert(4 & 3, 0)
assert(4 ^ 8, 12)
assert(100 % 8, 4)

assert(100 & 30 | 30 ^ 20, 14)
assert(30 ^ 100 & 20, 26)
