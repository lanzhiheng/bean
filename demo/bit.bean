assert(1 + 2 and 3, 3)
assert(1 + 2 or 0, 3)
assert(1 + 2 or false, 3)
assert(1 >> 3, 0)
assert(1 << 2, 4)
assert(1.3 << 4, 16)

assert(not (nil and true), true)
assert(not (1 + 2 + 3 and true), false)

assert(4 | 1, 5)
assert(4 & 3, 0)
assert(4 ^ 8, 12)
assert(100 % 8, 4)

assert(100 & 30 | 30 ^ 20, 14)
assert(30 ^ 100 & 20, 26)