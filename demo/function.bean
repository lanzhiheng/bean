fn sum1(i) {
  if (i > 1) {
    return sum1(i - 1) + i
  } else {
    return i
  }
}

print(sum1(4000))

fn sum2(i) {
  if (i > 1) {
    return sum2(i - 1) + i
  }
  return i
}

print(sum2(4000))

var a = [1,2,3,4,5,6]

var g = a.map(fn a(i) {
  if (i > 1) {
    return a(i - 1)
  }
  i * 2
})
print(g)