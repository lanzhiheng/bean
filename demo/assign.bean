assert(str.id, str['id'])

fn thisA() {
  return self.a
}

fn thisB() {
  return self.b
}

fn printA() {
  print(self.b)
}

thisObj = {
  retThis: fn() {
    return self
  }
}

temp = {
  a: 1,
  b: 2
}
temp['getA'] = thisA
temp['getB'] = thisB
temp.printA = printA
assert(thisObj.retThis(), thisObj)
assert(temp.getA(), 1)
assert(temp.getB(), 2)

fn globalSetting() {
  fn a() {
    global a = 100
  }
  a()
}

fn globalGetting() {
 assert(global a, 100)
}

globalSetting()
globalGetting()

